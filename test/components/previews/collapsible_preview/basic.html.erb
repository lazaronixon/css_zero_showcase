    <div class="rounded-md border pi-4 pb-3 mbs-2 text-sm">
      @rails/jbuilder
    </div>
    <div class="rounded-md border pi-4 pb-3 mbs-2 text-sm">
      @rails/jsbundling-rails
    </div>
  </details>
</div>